        "ModelsNamespace": "Umb9.Core.UmbracoModels"
      },
      "Global": {
        "Id": "669feafd-bd74-43b8-ac2f-0adc99c7d866",
        "Smtp": {
          "From": "someone@someplace.com",
          "Host": "localhost",
          "Port": 25,
