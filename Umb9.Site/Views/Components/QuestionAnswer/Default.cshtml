@model Umbraco.Cms.Core.Models.Blocks.BlockListItem
@{
    var content = Model.Content as Umb9.Core.UmbracoModels.QuestionAnswer;
}

<div>
    <div class="text-lg leading-6 font-medium text-gray-900">
        @content.Question
    </div>
    <div class="mt-2 prose text-base text-gray-500">
        @Html.Raw(content.Answer)
    </div>
</div>