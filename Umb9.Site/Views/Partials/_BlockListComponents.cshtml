@using Umb9.Core.Shared.Models
@model (Umbraco.Cms.Core.Models.Blocks.BlockListModel, int)
@{ 
    var model = Model.Item1;
    var websiteId = Model.Item2;
}
@if (model?.Any() == true)
{
    foreach (var block in model)
    {
        if (block?.ContentUdi == null) { continue; }
        @(await Component.InvokeAsync(block.Content.ContentType.Alias, new BlockListViewModel(block, websiteId)))
    }
}
